                                                                        <item name="getgo" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                <item name="souhoola" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/souhoola</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="souhoola" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                <item name="sympl" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/sympl</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="sympl" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                
                                                                 <item name="contact" xsi:type="array">																	
