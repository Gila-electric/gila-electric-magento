                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                <item name="aman" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/aman</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="aman" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>

                                                                 <item name="forsa" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/forsa</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="forsa" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                            </item>
                                                        </item>
                                                    </item>
                                                </item>
                                            </item>
                                        </item>
                                    </item>
                                </item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
