                                                <item name="payment" xsi:type="array">
                                                    <item name="children" xsi:type="array">
                                                        <item name="renders" xsi:type="array">
                                                            <item name="children" xsi:type="array">
                                                                <item name="valuaccept" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/valuaccept</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="valuaccept" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                <item name="online" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/online</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="online" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                               
                                                                <item name="wallet" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/wallet</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="wallet" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                <item name="kiosk" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/kiosk</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="kiosk" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                <item name="premium" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/premium</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="premium" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                <item name="installments" xsi:type="array">																	
                                                                    <item name="component" xsi:type="string">Accept_Payments/js/view/payment/installments</item>
                                                                    <item name="methods" xsi:type="array">
                                                                        <item name="installments" xsi:type="array">
                                                                            <item name="isBillingAddressRequired" xsi:type="boolean">true</item>
                                                                        </item>
                                                                    </item>
                                                                </item>
                                                                <item name="getgo" xsi:type="array">																	
